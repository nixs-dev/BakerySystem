MDBoxLayout:
	orientation: "horizontal"
	padding: 3, 3
				
	canvas.before:
		Color:
			rgba: get_color_from_hex("#333333")
		Line:
			width: 2
			rectangle: self.x, self.y, self.width, self.height
				
	MDFlatButton:
		id: button
		size_hint: 1, 1
		text: ""
		font_name: "Resources/font/main.ttf"
		background_color: 0, 0, 0, 0
		color: get_color_from_hex("#000000")