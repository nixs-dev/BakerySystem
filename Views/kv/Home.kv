#:import get_color_from_hex kivy.utils.get_color_from_hex

ScrollView:
	canvas.before:
		Color:
			rgba: get_color_from_hex('#D3D4D5')
		Rectangle:
			pos: self.pos
			size: self.size
                
	GridLayout:
		id: products_grid
		cols: 2
		spacing: 5, 5
		height: self.minimum_height
		row_default_height: 350
		size_hint_y: None